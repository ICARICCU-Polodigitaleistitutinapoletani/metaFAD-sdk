<?xml version="1.0" encoding="utf-8"?>
<model:Model
        xmlns:pnx="http://www.pinax.org/dtd/1.0/"
        xmlns:model="pinax.models.*/"
        model:tableName="schedaSemplificata"
        model:usePrefix="false"
        model:type="document"
        model:baseClass="metafad.common.models.ActiveRecordDocument">

    <model:Define>
        <model:Field name="fulltext" type="string" index="fulltext" onlyIndex="true" />

        <model:Field name="name" type="string" />
        <model:Field name="oldName" type="string" />
        <model:Field name="form" type="object" readFormat="false"/>

        <model:Field name="fieldJson" type="string" />
    </model:Define>
</model:Model>
